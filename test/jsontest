#!/usr/bin/env unfurl

# don't need any '::' since it's an s[ ]
s[ JSON J ] require-as

# example of constructing arg, etc
{ 1 ![ ]  J ::new  swap  .objToJson } :serialized

[
  'rc' 200
  'msg' [
    [ 'name' 'Tushar' 'email' 'x@y.x' ] %
    [ "jfjfjfkfjhfhjfhgf" 2 3 + 1 3 .. ]
    'END'
  ]
] %
serialized print
